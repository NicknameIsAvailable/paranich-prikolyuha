<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="ResTxtCsv.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>ResTxtCsv.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="ResTxtCsv.resx">
      <Generator>ResourceManager</Generator>
      <LastGenOutput>ResTxtCsv.Designer.cs</LastGenOutput>
      <CustomToolNamespace>ResTxtCsv</CustomToolNamespace>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="ResTempl1.rct">
      <CustomToolNamespace>ResTxtCsv</CustomToolNamespace>
      <Generator>ResourceManager</Generator>
    </None>
  </ItemGroup>

</Project>
